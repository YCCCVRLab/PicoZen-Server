{
  "version": 2,
  "name": "picozen-server",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/server.js"
    },
    {
      "source": "/admin(.*)",
      "destination": "/server.js"
    },
    {
      "source": "/images/(.*)",
      "destination": "/server.js"
    },
    {
      "source": "/files/(.*)",
      "destination": "/server.js"
    },
    {
      "source": "/(.*)",
      "destination": "/server.js"
    }
  ],
  "functions": {
    "server.js": {
      "maxDuration": 30
    }
  }
}