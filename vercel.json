{
  "version": 2,
  "name": "picozen-server",
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "PGSSLMODE": "no-verify",
    "NODE_TLS_REJECT_UNAUTHORIZED": "0"
  },
  "functions": {
    "server.js": {
      "maxDuration": 30
    }
  }
}